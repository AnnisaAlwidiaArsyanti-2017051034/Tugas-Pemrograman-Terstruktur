#include<bits/stdc++.h>
using namespace std;
bool Operator(char oper){
    if(oper=='+'||oper=='-'||oper=='*'||oper=='/' ||oper=='%'){
        return true;
    }
    else{
        return false;
    }
}
bool Operand(char ch){
    if (((ch>='a')&&(ch<='z'))||((ch>='A')&&(ch<='Z'))||((ch>='0')&&(ch<='9'))){
         return true;
    } else{
        return false;
    }
}
int prioritas(char a){
    if (a == '*' || a == '/'){
        return 2;
    }
        
    if (a == '+' || a == '-'){
        return 3;
    }
    return 1;  
}
int main(){
    string infix1, infix2, postfix1, postfix2;
    getline(cin, infix1);                     
    char tmp;
    stack <char> st;
    for (int i = 0; i < (int)infix1.length(); i++){
        if (isdigit(infix1[i])){
            infix2 += infix1[i];
        }
        switch (infix1[i]) {
            case '-':
                if (i == 0 && isdigit(infix1[i + 1])){
                    infix2 += "-";
                } else if(infix1[i+1]=='(' && isdigit(infix1[i - 2])){
                    infix2 += "-1 * ";
                } else if (isdigit(infix1[i + 1]) && !(isdigit(infix1[i - 1]))){
                    infix2 += "-1 * ";
                } else if (infix1[i - 2] != ')' && infix1[i - 1] != ')' && infix1[i + 1] == '(' && !(isdigit(infix1[3 - 2]))){
                    infix2 += "-1 * ";
                } else{
                    infix2 += " - ";
                }
                break;
            case '+':
                infix2 += " + ";
                break;
            case '*':
                infix2 += " * ";
                break;
            case '/':
                infix2 += " / ";
                break;
            case '%':
                infix2 += " % ";
                break;
            case '(':
                if(!(isdigit(infix1[i-1]))){
                    infix2 += "( ";
                } else{
                infix2 += " ( ";
                }
                break;
            case ')':
                if(!(isdigit(infix1[i+1]))){
                    infix2 += " )";
                } else{
                infix2 += " ) ";
                }
                break;
        }
       
    }
